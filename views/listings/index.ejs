<!-- views/index.ejs -->
<!DOCTYPE html>
<html>

<head>
    <title>All Listings</title>
</head>

<body>
    <h1>All Listings</h1>
    <ul>
        <% for(let listing of allListings) { %>
            <li><a href="/listings/<%=listing._id  %>">
                    <%= listing.title %>
                </a></li>
            <% } %>
    </ul>
</body>

</html>